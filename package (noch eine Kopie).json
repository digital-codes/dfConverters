{
  "name": "dfconverters",
  "version": "0.1.0",
  "description": "Converters for dflib dataframe library",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "doc": "npx typedoc --options typedoc.json",
    "start": "node dist/index.js",
    "prepare": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "canvas": "^2.11.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.11",
    "typedoc-plugin-markdown": "^4.2.10",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "dflib": "github:digital-codes/dflib"
  }
}